    "@material-ui/icons": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "draft-js": "^0.10.5",
    "draft-js-markdown-plugin": "^3.0.0",
    "draft-js-mathjax-plugin": "^1.3.4",
    "draft-js-plugins-editor": "^2.1.1",
    "electron-is-dev": "^0.3.0",
    "material-ui": "^1.0.0-beta.47",
